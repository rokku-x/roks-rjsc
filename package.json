{
  "name": "roks-rjsc",
  "version": "0.2.1",
  "description": "A React component library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "prepare": "npm run build",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "jsdom": "^27.4.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rokku-x/roks-rjsc.git"
  },
  "bugs": {
    "url": "https://github.com/rokku-x/roks-rjsc/issues"
  },
  "homepage": "https://github.com/rokku-x/roks-rjsc#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./modal": {
      "types": "./dist/modal.d.ts",
      "import": "./dist/modal.esm.js",
      "require": "./dist/modal.cjs.js"
    },
    "./loading": {
      "types": "./dist/loading.d.ts",
      "import": "./dist/loading.esm.js",
      "require": "./dist/loading.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "keywords": [
    "react",
    "component",
    "library"
  ],
  "author": "rokku",
  "license": "MIT"
}
